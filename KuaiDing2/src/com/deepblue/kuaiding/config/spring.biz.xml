<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="KdRestaurantBiz" class="com.deepblue.kuaiding.biz.KdRestaurantBiz">
		<property name="kdRestaurantDAO">
			<ref bean="KdRestaurantDAO" />
		</property>
	</bean>
	
	<bean id="KdRstinfoBiz" class="com.deepblue.kuaiding.biz.KdRstinfoBiz">
		<property name="kdRstinfoDAO">
			<ref bean="KdRstinfoDAO" />
		</property>
	</bean>
	
	<bean id="KdMenuBiz" class="com.deepblue.kuaiding.biz.KdMenuBiz">
		<property name="kdMenuDAO">
			<ref bean="KdMenuDAO" />
		</property>
	</bean>
	
</beans>